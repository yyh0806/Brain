# PointCloud2 Display (/front_3d_lidar/lidar_points)
Class: rviz/PointCloud2
Name: Nova Carter 3D LiDAR
Topic: /front_3d_lidar/lidar_points
Name: Nova_Carter_3D_LiDAR_Qt

# Size (m)
Class: rviz/PointCloud2
Name: Nova Carter LiDAR Size
Value: 20

# Style
Class: rviz/PointCloud2
Name: Nova Carter LiDAR Style
Value: Flat Shading

# Color Transformer
Class: rviz/PointCloud2
Name: Nova Carter Color Transformer
Value: AxisColor

# Position Transformer
Class: rviz/PointCloud2
Name: Nova Carter Position Transformer
Value: XYZ

# Auto Size
Class: rviz/PointCloud2
Name: Nova Carter Auto Size
Value: false

