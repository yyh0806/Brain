Panels:
  - Class: rviz_common/Displays
    Help Height: 480
    Name: Displays

  - Class: rviz_common/Displays
    Help Height: 480
    Name: Displays

Displays:
  - Class: nav_msgs/OccupancyGrid
    Help Height: 400
    Name: Semantic Occupancy Grid
    
  - Class: visualization_msgs/MarkerArray
    Help Height: 400
    Name: Semantic Objects
    
  - Class: visualization_msgs/MarkerArray
    Help Height: 400
    Name: Belief Markers
    
  - Class: nav_msgs/Path
    Help Height: 400
    Name: Robot Trajectory
    
  - Class: visualization_msgs/MarkerArray
    Help Height: 400
    Name: Exploration Frontiers
    
  - Class: visualization_msgs/MarkerArray
    Help Height: 400
    Name: Change Events
    
  - Class: visualization_msgs/MarkerArray
    Help Height: 400
    Name: VLM Detections

Visualization Manager:
  Class: rviz/VisualizationManager
  Name: Visualization Manager
  Global Options:
    Background Color: 48; 48; 48
    Fixed Frame: map
    Frame Rate: 30

Preferences:
  - Class: rviz_common/Preferences
    Name: Preferences

Tool Properties:
  - Class: rviz/Tool Properties
    Name: Tool Properties
    - Property tree style: 1
    - Property tree show all: 1
    - Show help: 1

