Panels:
  - Class: rviz_common/Displays
    Name: Displays
  - Class: rviz_common/Views
    Name: Views
  - Class: rviz_common/Selection
    Name: Selection
  - Class: rviz_common/Tool Properties
    Name: Tool Properties
  - Class: rviz_common/Time
    Name: Time
Visualization Manager:
  Global Options:
    Fixed Frame: map
    Frame Rate: 30
  Displays:
    # 1. 语义占据栅格
    - Name: Semantic Occupancy Grid
      Class: rviz_default_plugins/Map
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /world_model/semantic_grid
      Color Scheme: map
      Draw Behind: false
      Alpha: 0.7
      Update Threshold: 1
      Resolution: 0.1

    # 2. 语义物体标签
    - Name: Semantic Labels
      Class: rviz_default_plugins/MarkerArray
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /world_model/semantic_markers
      Marker Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /world_model/semantic_markers

    # 3. 机器人轨迹
    - Name: Robot Trajectory
      Class: rviz_default_plugins/Path
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /world_model/trajectory
      Color: 0; 255; 0
      Line Style: Lines
      Line Width: 0.1
      Offset:
        X: 0
        Y: 0
        Z: 0

    # 4. 探索前沿
    - Name: Exploration Frontiers
      Class: rviz_default_plugins/MarkerArray
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /world_model/frontiers
      Marker Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /world_model/frontiers

    # 5. TF树
    - Name: TF
      Class: rviz_default_plugins/TF

    # 6. Axes（显示地图原点）
    - Name: Map Origin
      Class: rviz_default_plugins/Axes
      Reference Frame: map
      Length: 10.0
      Radius: 1.0

  Views:
    Current:
      Class: rviz_default_plugins/Orbit
      Distance: 50.0
      Focal Point:
        X: 0
        Y: 0
        Z: 0
      Focal Shape Size: 0.05
      Name: Current View
      Pitch: 1.4
      Target Frame: map
      Yaw: 0.785398
    Saved: ~
  Window Geometry:
    "":
      collapsed: false
    Height: 846
    Hide Left Dock: false
    Hide Right Dock: false
    QMainWindow State: 
  Width: 1200
  X: 100
  Y: 100
